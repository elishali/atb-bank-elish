    <div class="Calculation">
        <button class="subscribe" id="cash" onclick="OpenCash()">
            <div class="row">
                <div class="col-lg-6">
                    <i class="fa-solid fa-money-check-dollar"></i>
                </div>
                <div class="col-lg-6">
                    <p>nağd krediti hesabla</p>
                </div>
            </div>
        </button>
        <button class="subscribe" id="deposit" onclick="OpenDeposit()">
            <div class="row">
                <div class="col-lg-6">
                    <i class="fa-solid fa-vault"></i>
                </div>

                <div class="col-lg-6">
                    <p>əmanət üzrə gəliri hesabla</p>
                </div>
            </div>
        </button>

        <div class="cash_main" id="cash_main">
            <h2>nağd krediti hesabla</h2>
            <div class="cash">
                <div class="row">
                    <div class="col-lg-8">
                        <div class="cash_left">
                            <div class="main">
                                <div class="input">
                                    <input id="input1" type="text" />
                                    <p>hansı məbləğ arzunuzu reallaşdırar?</p>
                                </div>
                                <div class="range">
                                    <input id="range1"
                                           type="range"
                                           min="1"
                                           max="35000"
                                           value="0"
                                           step="1" />
                                </div>
                            </div>
                            <div class="main">
                                <div class="input">
                                    <input id="input2" type="text" />
                                    <p>müddəti seçin</p>
                                </div>
                                <div class="range">
                                    <input id="range2"
                                           type="range"
                                           min="1"
                                           max="59"
                                           value="0"
                                           step="1" />
                                </div>
                            </div>
                            <div class="main">
                                <div class="input">
                                    <input id="input3" type="text" />
                                    <p>faiz dərəcəsi (%)</p>
                                </div>
                                <div class="range">
                                    <input id="range3"
                                           type="range"
                                           min="11.9"
                                           max="20.9"
                                           value="0"
                                           step="0.1" />
                                </div>
                            </div>
                            <div class="main_last">
                                <div class="row">
                                    <div class="col-lg-1">
                                        <label class="cus-checkbox__label">
                                            <input type="checkbox"
                                                   class="cus-checkbox__input"
                                                   checked="" />
                                            <span class="cus-checkbox__check"></span>
                                        </label>
                                    </div>
                                    <div class="col-lg-11">
                                        <p>
                                            "sifariş" düyməsini basaraq
                                            <u>
                                                <a href="./security.html">müraciət qaydaları</a>
                                            </u>
                                            ilə razılaşmış olursunuz
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 cash_right">
                        <p>Bizim təklif</p>
                        <div class="info">
                            <p>Aylıq ödəniş</p>
                            <h2 id="pertotal">₼</h2>
                        </div>
                        <div class="info">
                            <p>Ümumi məbləğ</p>
                            <h2 id="total">₼</h2>
                        </div>
                        <div class="button">
                            <button id="button">hesabla</button>
                        </div>
                        <div class="button" id="cash_order_btn">
                            <button id="OpenCashOrder" onclick="OpenCashOrder()">
                                sifariş
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="cash_order" id="cash_order">
            <h2>onlayn muraciət</h2>
            <p>istənilən məqsəd üçün nağd kredit</p>
            <div class="row">
                <div class="col-lg-6">
                    <input type="text"
                           placeholder="kredit məbləği (maks. 30 000 azn)" />
                </div>
                <div class="col-lg-6">
                    <input type="text" placeholder="kredit müddəti (48 ayadək)" />
                </div>
            </div>
            <div class="city">
                <select>
                    <option value="Şəhər">Şəhər</option>
                    <option value="Bakı">Bakı</option>
                    <option value="Şirvan">Şirvan</option>
                    <option value="Gəncə">Gəncə</option>
                    <option value="Naxçıvan">Naxçıvan</option>
                </select>
                <hr />
            </div>
            <p>şəxsi məlumat</p>
            <div class="row">
                <div class="col-lg-6">
                    <input type="text" placeholder="ad, soyad" />
                </div>
                <div class="col-lg-6">
                    <input type="text" placeholder="fin kod" />
                </div>
            </div>
            <div class="row">
                <div class="col-lg-6">
                    <input type="tel" placeholder="telefon" />
                </div>
                <div class="col-lg-6">
                    <input type="text" placeholder="email" />
                </div>
            </div>
            <div class="row">
                <div class="col-lg-6">
                    <input type="text" placeholder="iş yeri (qurumun adı)" />
                </div>
                <div class="col-lg-6">
                    <input type="text" placeholder="kredit müddəti (48 ayadək)" />
                </div>
            </div>
            <div class="row">
                <div class="col-lg-6">
                    <input type="text" placeholder="iş stajı (ay)" />
                </div>
                <div class="col-lg-6">
                    <input type="text" placeholder="rəsmi gəlit (rəqəmlə)" />
                </div>
            </div>
            <input type="text"
                   placeholder="digər banklara olan aylıq ödəniş (əgər varsa) və ya zaminlik" />

            <div class="cash_order_last">
                <div class="row">
                    <div class="col-lg-1">
                        <label class="cus-checkbox__label">
                            <input type="checkbox"
                                   class="cus-checkbox__input"
                                   checked="" />
                            <span class="cus-checkbox__check"></span>
                        </label>
                    </div>
                    <div class="col-lg-11">
                        <p>
                            məlumatlarımın Asan Finansdan əldə edilməsi və AKB reyestrində
                            sorğu edilməsinə razıyam
                        </p>
                    </div>
                </div>
            </div>

            <div class="button">
                <button id="button">sifariş</button>
            </div>

            <p>*bütün bəndlərin doldurulması mütləqdir</p>
        </div>

        <div class="deposit_main" id="deposit_main">
            <h2>əmanət üzrə gəliri hesablayın</h2>
            <div class=" deposit">
                <div class="row">
                    <div class="col-lg-8">
                        <div class="row">
                            <div class="col-lg-6">
                                <select>
                                    <option value="valyuta">valyuta</option>
                                    <option value="AZN">AZN</option>
                                    <option value="USD">USD</option>
                                </select>
                            </div>
                            <div class="col-lg-6">
                                <select>
                                    <option value="faizlərin ödənilməsi">
                                        faizlərin ödənilməsi
                                    </option>
                                    <option value="müddətin sonunda">müddətin sonunda</option>
                                    <option value="aylıq ödəniş">aylıq ödəniş</option>
                                </select>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-6">
                                <select>
                                    <option value="valyuta">müddət</option>
                                    <option value="12">12</option>
                                    <option value="24">24</option>
                                    <option value="36">36</option>
                                </select>
                            </div>
                            <div class="col-lg-6">
                                <input type="text"
                                       placeholder="məbləğ (min. 1000 AZN/USD)*" />
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 deposit_interest">
                        <div class="periodly d-none">
                            <p>faizlər - müddətin sonunda</p>
