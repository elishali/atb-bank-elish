
@{
    ViewData["Title"] = "Atb_card_premium";
}

<div class="other_page_banner">
    <img src="~/assets/images/atbcard_premium_header.png"
         style="width: 100%" />
    <div class="text">
        <h2>atb premium</h2>
        <p>kartı</p>
        <button class="banner_btn subscribe"><a href="~/#">sifariş</a></button>
    </div>
</div>

<div class="maincontainer">

    <div class="Cards_Two">
        <div class="row">
            <div class="col-lg-3 card_info">
                <h5>AZN/USD/EUR/GBP</h5>
                <p>3 illik – 300 AZN</p>
            </div>
            <div class="col-lg-3 card_info">
                <h5>15 000 ilkin mədaxildə</h5>
                <p>kart hədiyyə</p>
            </div>
            <div class="col-lg-3 card_info">
                <h5>
                    AZN kartlarında <br />
                    qalığa gəlir
                </h5>
                <p>5,5%</p>
            </div>
            <div class="col-lg-3 card_info">
                <h5>kartdan karta</h5>
                <p>0,5%</p>
            </div>
        </div>
    </div>

    <div class="Superiority">
        <div class="row Superiority_Bottom">
            <div class="col-lg-6">
                <div class="Superiority_One" id="insurance">
                    <div class="row">
                        <div class="col-lg-8">
                            <h4><b>sığorta</b></h4>
                            <p>
                                kart sahibi və ailəsi üçün <br />
                                səyahət sığortası <br />
                                Mastercard - 115,000 EUR <br />
                                VISA - 2,500,000 USD
                            </p>
                        </div>
                        <div class="col-lg-4">
                            <img src="~/assets/images/atbcard_premium_ok.png" alt="" />
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="Superiority_Two" id="profitable">
                    <div class="row">
                        <div class="col-lg-8">
                            <h4><b>sərfəli</b></h4>
                            <p>
                                əyləncə, restoran, alış-veriş, <br />
                                otel və nəqliyyat üzrə 30%- <br />
                                dək endirimlər
                            </p>
                        </div>
                        <div class="col-lg-4">
                            <img src="~/assets/images/atbcard_premium_30.png" alt="" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="Superiority">
        <div class="Superiority_Top">
            <div class="row">
                <div class="col-lg-6">
                    <h4><b>kart hədiyyə</b></h4>
                    <b>3 years</b> – 15,000 AZN/USD/EUR/GBP in initial income
                </div>
                <div class="col-lg-6">
                    <img src="~/assets/images/atbcard_premium_income.png" alt="" />
                </div>
            </div>
        </div>
    </div>

    <div class="Online_Order">
        <div class="cash_order_last">
            <div class="row">
                <div class="col-lg-1">
                    <label class="cus-checkbox__label">
                        <input type="checkbox" class="cus-checkbox__input" checked="">
                        <span class="cus-checkbox__check"></span>
                    </label>
                </div>
                <div class="col-lg-11">
                    <p>
                        ilkin mədaxil etmək istəyirsiniz?
                    </p>
                </div>
            </div>
        </div>
        <select class="wpcf7-form-control wpcf7-select wpcf7-validates-as-required select2-hidden-accessible" aria-required="true" aria-invalid="false" name="filial" data-select2-id="select2-data-1-0nsd" tabindex="-1" aria-hidden="true">
            <option value="" data-select2-id="select2-data-3-4yzf">kartın növü</option>
            <option value="Müştəri xidmətləri mərkəzi" data-select2-id="select2-data-22-suop"> Visa infinite</option>
            <option value="Müştəri xidmətləri mərkəzi" data-select2-id="select2-data-22-suop"> Mastercard world black edition</option>
        </select>

        <select class="wpcf7-form-control wpcf7-select wpcf7-validates-as-required select2-hidden-accessible" aria-required="true" aria-invalid="false" name="filial" data-select2-id="select2-data-1-0nsd" tabindex="-1" aria-hidden="true">
            <option value="" data-select2-id="select2-data-3-4yzf">valyuta</option>
            <option value="Müştəri xidmətləri mərkəzi" data-select2-id="select2-data-22-suop">AZN</option>
            <option value="Müştəri xidmətləri mərkəzi" data-select2-id="select2-data-22-suop">USD</option>
            <option value="Müştəri xidmətləri mərkəzi" data-select2-id="select2-data-22-suop">EURO</option>
            <option value="Müştəri xidmətləri mərkəzi" data-select2-id="select2-data-22-suop">GPB</option>
        </select>


        <input type="text" placeholder="çatdırılma ünvanı">
        <p>*çatdırılma yalnız Bakı şəhəri üzrədir</p>

        <h4>şəxsi məlumat</h4>
        <input type="text" placeholder="ad, soyad">
        <div class="row">
            <div class="col-lg-6">
                <input type="tel" placeholder="telefon">
            </div>
            <div class="col-lg-6">
                <input type="email" placeholder="email">
            </div>
        </div>

        <input type="text" placeholder="fin kod">


        <div class="cash_order_last">
            <div class="row">
                <div class="col-lg-1">
                    <label class="cus-checkbox__label">
                        <input type="checkbox" class="cus-checkbox__input" checked="">
                        <span class="cus-checkbox__check"></span>
                    </label>
                </div>
                <div class="col-lg-11">
                    <p>"göndər" düyməsini basaraq <a href="~/security.html">müraciət qaydaları</a>  ilə razılaşmış olursunuz</p>
                </div>
            </div>
        </div>

        <div class="button">
            <button id="button">göndər</button>
        </div>

        <p>*bütün bəndlərin doldurulması mütləqdir</p>

    </div>

    <div class="Superiority">
        <div class="Superiority_Top"
             style="background-image: url(../assets/images/consumption_credit.jpg)">
            <div class="row">
                <div class="col-lg-10 easy_text">
                    <h2>
                        <b>
                            rahat və müasir atb360 mobil <br />
                            tətbiqi
                        </b>
                    </h2>
                    <p>
                        rahat və güvənli atb360 mobil tətbiqi ilə <br />
                        bütün bank əməliyyatlarını smartfonunuzdan <br />
                        aparın
                    </p>
                </div>
                <div class="col-lg-2 easy_app">
                    <p>
                        tətbiqi <br />
                        yükləyin
                    </p>
                    <a href="~/consumption_credit.html" target="_blank">
                        <img src="~/assets/images/qr.svg" alt="" />
                    </a>
                    <br />
                    <a href="~/consumption_credit.html" target="_blank">
                        <img src="~/assets/images/app.svg" alt="" />
                    </a>
                </div>
            </div>
        </div>
    </div>

    <div class="Useful_Info">
        <h4>faydalı məlumat</h4>
        <div class="interest_rates">
            <div class="row">
                <div class="col-lg-11" id="interest_rates_tittle">
                    faiz dərəcələri
                </div>
                <div class="col-lg-1">
                    <button class="subscribe"
                            onclick="Open_interest_rates_menu()"
                            id="Open_interest_rates_menu">
                        <i class="fa-solid fa-plus" id="openiconcon"></i>
                    </button>
                </div>
            </div>
            <div class="interest_rates_menu" id="interest_rates_menu">
                <table>
                    <tr>
                        <th>müddət</th>
                        <th>faiz dərəcəsi</th>
                        <th>xidmət haqqı</th>
                    </tr>
                    <tr>
                        <td>12 ayadək</td>
                        <td>11.5%</td>
                        <td>0%</td>
                    </tr>
                    <tr>
                        <td>24 ayadək</td>
                        <td>13.5%</td>
                        <td>0%</td>
                    </tr>
                    <tr>
                        <td>36 ayadək</td>
                        <td>14%</td>
                        <td>0.5% (min. 50 AZN)</td>
                    </tr>
                    <tr>
                        <td>48 ayadək</td>
                        <td>15%</td>
                        <td>1% (min. 50 AZN)</td>
                    </tr>
                </table>
            </div>
        </div>

        <div class="conditions">
            <div class="row">
                <div class="col-lg-11" id="contittle">şərtlər</div>
                <div class="col-lg-1">
                    <button class="subscribe"
                            onclick="OpenConditionMenu()"
                            id="OpenConditionMenu">
                        <i class="fa-solid fa-plus" id="openiconcon"></i>
                    </button>
                </div>
            </div>
            <div class="condition_menu" id="condition_menu">
                <ul>
                    <li>məbləğ: <b>300 – 30 000 AZN</b></li>
                    <li>30 000 AZN-dək <b>zaminsiz</b></li>
                    <li>valyuta: <b>AZN</b></li>
                    <li>müddət: <b>3 – 48 ay</b></li>
                    <li>ödəniş şərti: <b>annuitet</b></li>
                    <li>sığorta: <b>tələb olunmur</b></li>
                </ul>
            </div>
        </div>

        <div class="requirements">
            <div class="row">
                <div class="col-lg-11" id="requirementstittle">tələblər</div>
                <div class="col-lg-1">
                    <button class="subscribe"
                            onclick="OpenrequirementsMenu()"
                            id="OpenrequirementsMenu">
                        <i class="fa-solid fa-plus" id="openiconcon"></i>
                    </button>
                </div>
            </div>
            <div class="requirements_menu" id="requirements_menu">
                <ul>
                    <li>yaş həddi: <b>18-70</b></li>
                    <li>
                        iş yeri<b> rəsmi iş yeri olan istənilən şəxs / təqaüdçü</b>
                    </li>
                    <li>
                        iş stajı:
                        <b>6 ay, sonuncu iş yerində fasiləsiz keçidlə minimum 3 ay</b>
                    </li>
                    <li>əməkhaqqı: <b>minimum tələb yoxdur</b></li>
                </ul>
            </div>
        </div>
    </div>
</div>

